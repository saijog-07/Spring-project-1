<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- Enable annotation-based configuration -->
    <context:annotation-config />
 
	
	<bean id="s1" class="com.project.SpringProject.Student">
    
    	<property name="rollno" value="201"/>
    	<property name="name" value="Ajay"/>
    	<property name="fee" value="2000"/>

	</bean>
	
	<bean id="add1" class="com.project.SpringProject.Address"   name="home">
		<property name="hno" value="540"/>
    	<property name="city" value="Karad"/>
    	<property name="pincode" value="415124"/>
	</bean>
	 
	<bean id="add2" class="com.project.SpringProject.Address" name="university">
		<property name="hno" value="123"/>
    	<property name="city" value="Kolhapue"/>
    	<property name="pincode" value="410410"/>
	</bean>
	
	
</beans>




